{
  "title": "JIS to US",
  "rules": [
    {
      "description": "Send @ when press shift+2",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "2",
            "modifiers": {
              "mandatory": [
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "open_bracket"
            }
          ]        
        },
        {
          "type": "basic",
          "from": {
            "key_code": "open_bracket"
          },
          "to": [
            {
              "key_code": "7",
              "modifiers": [
                "shift"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Send ' when press @",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "open_bracket"
          },
          "to": [
            {
              "key_code": "7",
              "modifiers": [
                "shift"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Send double quote when press shift+@",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "open_bracket",
            "modifiers": {
              "mandatory": [
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "2",
              "modifiers": [
                "shift"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Send apostrophy when press shift+7",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "7",
            "modifiers": {
              "mandatory": [
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "open_bracket",
              "modifiers": [
                "shift"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Send backslash when press shift+_",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "international1",
            "modifiers": {
              "mandatory": [
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "international3",
              "modifiers": [
                "option"
              ]
            }
          ]
        }
      ]
    }
  ]
}

